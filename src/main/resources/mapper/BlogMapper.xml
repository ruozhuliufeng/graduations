<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cslg.graduation.mapper.BlogMapper">
   <select id="findMaxBlogs" resultType="com.cslg.graduation.entity.Blog">
       SELECT *
        FROM blog
        ORDER BY hits DESC
        LIMIT 10
   </select>
</mapper>