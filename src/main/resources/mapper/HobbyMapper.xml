<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cslg.graduation.mapper.HobbyMapper">
   <select id="findMaxhobby" resultType="com.cslg.graduation.entity.Hobby">
       SELECT *
        FROM hobby
        ORDER BY hits DESC
        LIMIT 1
   </select>
</mapper>